{
  "framework": "nextjs",
  "redirects": [
    {
      "source": "/blog",
      "destination": "/travel-guides",
      "permanent": true
    },
    {
      "source": "/blog/:path*",
      "destination": "/travel-guides/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate"
        }
      ]
    }
  ]
}
